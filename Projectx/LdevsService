-- Ldevs Software V5.23 GUI with Old Version Support
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local localPlayer = Players.LocalPlayer

-- First Notification
StarterGui:SetCore("SendNotification", {
    Title = localPlayer.Name,
    Text = "Ldevs Software V5.23",
    Duration = 60,
    Icon = "rbxthumb://type=Asset&id=0&w=150&h=150"
})

task.delay(60, function()
    StarterGui:SetCore("SendNotification", {
        Title = "wearedevs.net/Ldevs Has Been Updated",
        Text = "Ldevs Software Has Been Injected",
        Duration = 20,
        Icon = "rbxthumb://type=Asset&id=0&w=150&h=150"
    })

    task.delay(20, function()
        -- Create Toggle Button After Notifications
        local ToggleGui = Instance.new("ScreenGui", game.CoreGui)
        ToggleGui.Name = "ToggleExecutorGui"

        local OpenButton = Instance.new("TextButton", ToggleGui)
        OpenButton.Size = UDim2.new(0, 200, 0, 50)
        OpenButton.Position = UDim2.new(0.5, -100, 0.9, -25)
        OpenButton.Text = "Open Ldevs Software"
        OpenButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
        OpenButton.TextColor3 = Color3.new(1, 1, 1)
        OpenButton.Font = Enum.Font.SourceSansBold
        OpenButton.TextSize = 18
        OpenButton.Visible = true

        OpenButton.MouseButton1Click:Connect(function()
            ToggleGui:Destroy()

            -- Create Updated GUI
            local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
            ScreenGui.Name = "CustomExecutor"

            local Frame = Instance.new("Frame", ScreenGui)
            Frame.Size = UDim2.new(0, 320, 0, 300)
            Frame.Position = UDim2.new(0.5, -160, 0.5, -150)
            Frame.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
            Frame.BorderSizePixel = 0
            Frame.Active = true
            Frame.Draggable = true

            local Title = Instance.new("TextLabel", Frame)
            Title.Size = UDim2.new(1, 0, 0, 30)
            Title.Text = "Lvdevs v5"
            Title.BackgroundTransparency = 1
            Title.TextColor3 = Color3.new(1, 1, 1)
            Title.Font = Enum.Font.SourceSansBold
            Title.TextSize = 20

            local InfoLabel = Instance.new("TextLabel", Frame)
            InfoLabel.Size = UDim2.new(1, -20, 0, 30)
            InfoLabel.Position = UDim2.new(0, 10, 0, 30)
            InfoLabel.Text = "Executor by LunarifyDevelopes. Bypasses anti-cheat."
            InfoLabel.BackgroundTransparency = 1
            InfoLabel.TextWrapped = true
            InfoLabel.TextColor3 = Color3.new(1, 1, 1)
            InfoLabel.Font = Enum.Font.SourceSans
            InfoLabel.TextSize = 14

            local TextBox = Instance.new("TextBox", Frame)
            TextBox.Size = UDim2.new(1, -20, 0, 120)
            TextBox.Position = UDim2.new(0, 10, 0, 65)
            TextBox.Text = "-- Enter script here"
            TextBox.MultiLine = true
            TextBox.ClearTextOnFocus = false
            TextBox.TextWrapped = true
            TextBox.TextYAlignment = Enum.TextYAlignment.Top
            TextBox.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
            TextBox.TextColor3 = Color3.new(1, 1, 1)
            TextBox.Font = Enum.Font.Code
            TextBox.TextSize = 14

            local Execute = Instance.new("TextButton", Frame)
            Execute.Size = UDim2.new(1, -20, 0, 30)
            Execute.Position = UDim2.new(0, 10, 1, -70)
            Execute.Text = "Execute"
            Execute.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
            Execute.TextColor3 = Color3.new(1, 1, 1)
            Execute.Font = Enum.Font.SourceSansBold
            Execute.TextSize = 18

            Execute.MouseButton1Click:Connect(function()
                local code = TextBox.Text
                local func, err = loadstring(code)
                if func then
                    pcall(func)
                else
                    warn("Error loading script: " .. tostring(err))
                end
            end)

            local OldVersionButton = Instance.new("TextButton", Frame)
            OldVersionButton.Size = UDim2.new(1, -20, 0, 30)
            OldVersionButton.Position = UDim2.new(0, 10, 1, -35)
            OldVersionButton.Text = "Old Beta Version"
            OldVersionButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            OldVersionButton.TextColor3 = Color3.new(1, 1, 1)
            OldVersionButton.Font = Enum.Font.SourceSans
            OldVersionButton.TextSize = 16

            OldVersionButton.MouseButton1Click:Connect(function()
                if ScreenGui then ScreenGui:Destroy() end

                StarterGui:SetCore("SendNotification", {
                    Title = localPlayer.Name,
                    Text = "Ldevs Software V5.23",
                    Duration = 60,
                    Icon = "rbxthumb://type=Asset&id=0&w=150&h=150"
                })

                task.delay(60, function()
                    StarterGui:SetCore("SendNotification", {
                        Title = "wearedevs.net/Ldevs Has Been Updated",
                        Text = "Ldevs Software Has Been Injected",
                        Duration = 20,
                        Icon = "rbxthumb://type=Asset&id=0&w=150&h=150"
                    })

                    task.delay(20, function()
                        local OldGui = Instance.new("ScreenGui", game.CoreGui)
                        OldGui.Name = "OldLdevsGUI"

                        local OldFrame = Instance.new("Frame", OldGui)
                        OldFrame.Size = UDim2.new(0, 320, 0, 260)
                        OldFrame.Position = UDim2.new(0.5, -160, 0.5, -130)
                        OldFrame.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
                        OldFrame.Active = true
                        OldFrame.Draggable = true

                        local OldTitle = Instance.new("TextLabel", OldFrame)
                        OldTitle.Size = UDim2.new(1, 0, 0, 30)
                        OldTitle.Text = "Lvdevs v5"
                        OldTitle.BackgroundTransparency = 1
                        OldTitle.TextColor3 = Color3.new(1, 1, 1)
                        OldTitle.Font = Enum.Font.SourceSansBold
                        OldTitle.TextSize = 20

                        local OldTextBox = Instance.new("TextBox", OldFrame)
                        OldTextBox.Size = UDim2.new(1, -20, 0, 120)
                        OldTextBox.Position = UDim2.new(0, 10, 0, 40)
                        OldTextBox.Text = "-- Old Version Script Area"
                        OldTextBox.MultiLine = true
                        OldTextBox.ClearTextOnFocus = false
                        OldTextBox.TextWrapped = true
                        OldTextBox.TextYAlignment = Enum.TextYAlignment.Top
                        OldTextBox.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
                        OldTextBox.TextColor3 = Color3.new(1, 1, 1)
                        OldTextBox.Font = Enum.Font.Code
                        OldTextBox.TextSize = 14

                        local OldExecute = Instance.new("TextButton", OldFrame)
                        OldExecute.Size = UDim2.new(1, -20, 0, 30)
                        OldExecute.Position = UDim2.new(0, 10, 1, -40)
                        OldExecute.Text = "Execute"
                        OldExecute.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
                        OldExecute.TextColor3 = Color3.new(1, 1, 1)
                        OldExecute.Font = Enum.Font.SourceSansBold
                        OldExecute.TextSize = 18

                        OldExecute.MouseButton1Click:Connect(function()
                            local code = OldTextBox.Text
                            local func, err = loadstring(code)
                            if func then
                                pcall(func)
                            else
                                warn("Error loading script: " .. tostring(err))
                            end
                        end)
                    end)
                end)
            end)
        end)
    end)
end)
