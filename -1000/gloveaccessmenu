-- Scripts

local function PAVHU_fake_script() -- edgelord.Script 
	local script = Instance.new('Script', edgelord)

	function onButtonClicked()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/IncognitoScripts/SlapBattles/refs/heads/main/Edgelord"))()
	end
	
	script.Parent.MouseButton1Click:connect(onButtonClicked)
	
end
coroutine.wrap(PAVHU_fake_script)()
local function AGATYPK_fake_script() -- kingSbeve.Script 
	local script = Instance.new('Script', kingSbeve)

	function onButtonClicked()
		game:WaitForChild("ReplicatedStorage").Sbeve:FireServer()
	end
	
	script.Parent.MouseButton1Click:connect(onButtonClicked)
	
end
coroutine.wrap(AGATYPK_fake_script)()
local function VGYQ_fake_script() -- Train.Script 
	local script = Instance.new('Script', Train)

	function onButtonClicked()
		game:WaitForChild("ReplicatedStorage").busmoment:FireServer()
	end
	
	script.Parent.MouseButton1Click:connect(onButtonClicked)
	
end
coroutine.wrap(VGYQ_fake_script)()
local function GEBNABT_fake_script() -- TheControlOfAl.Script 
	local script = Instance.new('Script', TheControlOfAl)

	function onButtonClicked()
		game:WaitForChild("ReplicatedStorage").brickl:FireServer()
	end
	
	script.Parent.MouseButton1Click:connect(onButtonClicked)
	
end
coroutine.wrap(GEBNABT_fake_script)()
local function ZMEAA_fake_script() -- TheControlOfAl_2.Script 
	local script = Instance.new('Script', TheControlOfAl_2)

	function onButtonClicked()
		game.StarterGui.GloveAccressss:Destroy()
	end
	
	script.Parent.MouseButton1Click:connect(onButtonClicked)
	
end
coroutine.wrap(ZMEAA_fake_script)()
