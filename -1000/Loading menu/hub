-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(17, 17, 17)
Frame.BackgroundTransparency = 1.000
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.32083115, 0, 0.466609597, 0)
Frame.Size = UDim2.new(0, 261, 0, 31)
Frame.Active = true
Frame.Draggable = true

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Size = UDim2.new(0, 264, 0, 30)
TextLabel.Font = Enum.Font.Merriweather
TextLabel.Text = "Glove Abusers & Mastery Abusers Hub"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextSize = 14.000
TextLabel.TextStrokeColor3 = Color3.fromRGB(8, 17, 75)
TextLabel.TextStrokeTransparency = 0.000
TextLabel.TextTransparency = 1.000

-- Scripts:

local function GWXP_fake_script() -- TextLabel.Script 
	local script = Instance.new('Script', TextLabel)

	script.Parent.TextTransparency = 1
	wait(0.9)
	script.Parent.TextTransparency = 0.9
	wait(0.8)
	script.Parent.TextTransparency = 0.8
	wait(0.7)
	script.Parent.TextTransparency = 0.7
	wait(0.6)
	script.Parent.TextTransparency = 0.6
	wait(0.5)
	script.Parent.TextTransparency = 0.5
	wait(0.4)
	script.Parent.TextTransparency = 0.4
	wait(0.3)
	script.Parent.TextTransparency = 0.3
	wait(0.2)
	script.Parent.TextTransparency = 0.2
	wait(0.1)
	script.Parent.TextTransparency = 0
end
coroutine.wrap(GWXP_fake_script)()
local function JXRV_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame)

	script.Parent.Transparency = 1
	wait(0.9)
	script.Parent.Transparency = 0.9
	wait(0.8)
	script.Parent.Transparency = 0.8
	wait(0.7)
	script.Parent.Transparency = 0.7
	wait(0.6)
	script.Parent.Transparency = 0.6
	wait(0.5)
	script.Parent.Transparency = 0.5
	wait(0.4)
	script.Parent.Transparency = 0.4
	wait(0.3)
	script.Parent.Transparency = 0.3
	wait(0.2)
	script.Parent.Transparency = 0.2
	wait(0.1)
	script.Parent.Transparency = 0
end
coroutine.wrap(JXRV_fake_script)()
